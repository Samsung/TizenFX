            inherit: "default" 0.0;
            color_class: "open_ux_theme";
            color: 255 255 255 255;
         }
         description { state: "disabled" 0.0;
            inherit: "default" 0.0;
            color: 0 0 0 25;
         }
      }
      part { name: "down_icon";
         scale: 1;
         clip_to: "base";
         description { state: "default" 0.0;
            rel2.to_x: "pad.right";
            rel2.relative: 0.0 1.0;
            align: 1.0 0.5;
            fixed: 1 1;
