      part { name: "elm.swallow.content"; type: SWALLOW;
         scale: 1;
         clip_to: "base";
         description { state: "default" 0.0;
            min: 26 26;
            max: 26 26;
            fixed: 1 1;
            align: 0.0 0.5;
            rel1.to_x: "pad.text";
            rel1.relative: 1.0 0.5;
            rel2.to_x: "pad.text";
            rel2.relative: 1.0 0.5;
         }
      }
      part { name: "event"; type: RECT;
         description { state: "default" 0.0;
            color: 0 0 0 0;
            fixed: 1 1;
         }
         description { state: "disabled" 0.0;
            inherit: "default" 0.0;
            visible: 0;
         }
