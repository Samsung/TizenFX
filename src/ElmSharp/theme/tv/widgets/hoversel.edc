            text {
               size: HOVERSEL_FONT_SIZE;
               max: 1 0;
               text_class: "button";
               align: 0.0 0.5;
            }
         }
         description { state: "focus" 0.0;
            inherit: "default" 0.0;
            color: 255 255 255 255;
         }
         description { state: "visible" 0.0;
            inherit: "default" 0.0;
         }
         description { state: "disabled" 0.0;
            inherit: "default" 0.0;
